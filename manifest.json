{
    "name": "GitHub Release Info",
    "version": "0.0.1",
    "manifest_version": 2,
    "permissions": [
        "https://github.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://github.com/*/*"
            ],
            "js": [
                "source/repository.js"
            ],
            "css": [
                "source/css/styles.css"
            ]
        }
    ]
}
